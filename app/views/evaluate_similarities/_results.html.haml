.input_data
  .matrix
    .label
      First PWM
      = download_list ticket, {'pcm' => 'pcm_first.pcm', 'pwm' => 'pwm_first.pwm'}
    %pre= Bioinform::PWM.new(task_params.pwm_first).round(3)
    .logo= show_image(ticket, 'pcm_first_direct.png')
  .matrix
    .label
      Second PWM
      = download_list ticket, {'pcm' => 'pcm_second.pcm', 'pwm' => 'pwm_second.pwm'}
    %pre= Bioinform::PWM.new(task_params.pwm_second).round(3)
    .logo= show_image(ticket, "pcm_second_#{task_results.orientation}.png")

.result_data
  .similarity
    .label Similarity:
    .value= task_results.similarity
    
